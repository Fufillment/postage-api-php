{
  "name": "fulfillment/postage-api",
  "description": "A wrapper for Fulfillment's Postage API",
  "keywords": [
    "fulfillment",
    "fulfillment.com"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/Fufillment/postage-api-php",
  "authors":[
    {
      "name": "Matt Duncan"
    },
    {
      "name": "James Weston"
    }
  ],
  "require": {
    "respect/validation": "0.9.3",
    "vlucas/phpdotenv": "~1.0",
    "netresearch/jsonmapper": "^0.8.0",
    "fulfillment/api-wrapper": "dev-master",
    "foxxmd/php-utilities": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Fulfillment\\Postage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fulfillment\\Postage\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
